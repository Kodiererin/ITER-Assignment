#include<stdio.h>
#include<stdio.h>
int main(){
	int arr[3][4];
	int *p;
	 
	 p = arr[0];
	
	 for(int i=0 ; i<3 ; i++){
	 		for(int j=0  ; j<4 ; j++){
	 			scanf("%d",*(*(arr+i)+j));
	 		}
	 }
	 
	 printf("\n");
	 
	 // Display the array
	 
	 for(int i=0 ; i<3 ; i++){
	 		for(int j=0  ; j<4 ; j++){
	 			printf("%d",*(*(p+i)+j));
	 		}
	 }
	 
	
	return 0;
}
