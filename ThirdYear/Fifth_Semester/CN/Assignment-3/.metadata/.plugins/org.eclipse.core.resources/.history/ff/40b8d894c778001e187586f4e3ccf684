import java.util.*;
import java.net.*;

public class Objective_1 {
	public static void main(String[] args) {
    	System.out.println("--------------------------------------------------------------");
    	System.out.println("Name : Rohit kumar");
    	System.out.println("Registration Number : 2141011062");
    	System.out.println("Section  : C");
    	System.out.println("--------------------------------------------------------------");
		try {
			Scanner sc = new Scanner(System.in);
			System.out.println("Enter the Website Name ");
			String website = sc.nextLine();
			InetAddress getIp = InetAddress.getByName(website);
			System.out.println(getIp.getHostAddress());
			System.out.println(getIp.getHostName());
//			System.out.println(getIp.getLocalHost());
		}catch(Exception e) {
			e.printStackTrace();
		}
	}
}
