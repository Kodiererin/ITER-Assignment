import java.util.*;
import java.net.*;


public class Objective_3 {
	public static void main(String[] args) {
		boolean isIP4;
		boolean isIP6;
		try {
			System.out.println("Get the Version of IP Address whether it is Version 6 or not");
			InetAddress ipAddress = InetAddress.getLocalHost();
			System.out.println(ipAddress);
			System.out.println(ipAddress.toString());
			
		}catch(Exception e) {
			e.printStackTrace();
		}
		
	}
}
