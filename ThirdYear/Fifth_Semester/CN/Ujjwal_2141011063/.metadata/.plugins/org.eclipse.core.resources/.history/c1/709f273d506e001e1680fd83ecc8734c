import java.util.*;
import java.net.*;


public class Objective_3 {
	public static void main(String[] args) {
		boolean isIP4;
		boolean isIP6;
		try {
			System.out.println("Get the Version of IP Address whether it is Version 6 or not");
			String IPAddress = InetAddress.getLocalHost().toString();
			InetAddress inetAddr = InetAddress.getByName(IPAddress);
			System.out.println(inetAddr.getAllByName(IPAddress).toString());
			
		}catch(Exception e) {
			e.printStackTrace();
		}
		
	}
}
