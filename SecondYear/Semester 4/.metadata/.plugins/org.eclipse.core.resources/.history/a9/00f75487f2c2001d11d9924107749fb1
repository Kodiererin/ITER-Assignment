package March_15;

public class Day1_Introduction {
	public static void main(String[] args) {
		System.out.println("Hello World");
		count(10);
	}
	public static int count(int x) {
		int c = 0;
		int sum = 0;
		c+=1;
		for(int i=0 ; i<x ; i++) {
			c+=1;
			sum+=i;
			c+=1;
		}
		c+=1;
		c+=1;
		System.out.println("Time Complexity "+c);
		return sum;
	}
}
