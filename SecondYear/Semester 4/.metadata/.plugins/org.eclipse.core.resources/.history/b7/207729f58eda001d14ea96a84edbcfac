package April_14;

class Numbers{
	int num;
	int myNumber;
	Numbers(int num , int myNumber){
		this.myNumber = myNumber;
		this.num = num;
	}
}

public class d_bubbleSort_Absolute {
	public static void main(String[] args) {
		
	}
	public static void compute() {
		int[] arr = {9,1,8,2,7,3,6,4,5};
		int k=5;
		
		Numbers[] num = new Numbers[9];
		
		for(int i=0 ; i<arr.length ; i++) {
			num[i] = new Numbers(arr[i], k-arr[i]);
		}
		
//		Getting started with Bubble Sort
		for(int i=0 ; i<num.length ; i++) {
			for (int j=0 ; j<num.length ; j++) {
				if(num[i].num<num[j].num) {
					int temp = num[i].num;
					num[i].num = num[j].num;
					num[j].num = temp;
				}
			}
		}
		
		for(int i=0 ; num.length ; i++) {
			System.out.println(num[i].myNumber);
		}
	}
}
