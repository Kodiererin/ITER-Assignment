package March_31;
//	IMP Question.
public class permutationOfAllIntegers {
	public static void main(String[] args) {
		int[] arr = {1,2,3};
		anagram(arr, 0, 2);
	}
	public static void anagram(int[] arr , int i , int j) {
		
		for(int k=i ; k<j ; k++) {
			System.out.println(arr.toString());
			arr = swap(arr,i,k);
			System.out.println(arr.toString());
			anagram(arr,i+1,j);
			arr = swap(arr,i,k);
		}
	}
	public static int[] swap(int[] arr , int a , int b) {
		int temp = arr[a];
		arr[a] = arr[b];
		arr[b] = temp;
		return arr;
	}
}
