package April_14;

public class a_Arrange012 {
	public static void main(String[] args) {
		arrange();
	}
//	Arrange 0,1,2
	public static void arrange() {
		int[] arr = {0,2,2,2,2,1,1,1,1,2,2,2,0,1,1,0,0,0};
		int left = arr[0];
		int right = arr[arr.length-1]; 
		for(int i=0 ; i<arr.length ; i++) {
			if(arr[i]==0) {
				int temp = left;
				left = arr[i];
				arr[i] = temp;
				left++;
			}else if(arr[i]==2) {
				int temp = right;
				right = arr[i];
				arr[i] = temp;
				right--;
			}
		}
		
		for(int i=0 ; i<arr.length ; i++) {
			System.out.println(arr[i]);
		}
	}
}
