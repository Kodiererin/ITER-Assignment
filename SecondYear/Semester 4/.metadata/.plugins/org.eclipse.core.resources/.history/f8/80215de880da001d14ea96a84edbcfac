package April_14;

public class a_Arrange012 {
	public static void main(String[] args) {
		arrange();
	}
//	Arrange 0,1,2
	public static void arrange() {
		int[] arr = {0,2,2,2,2,1,1,1,1,2,2,2,0,1,1,0,0,0};
		int left = 0;
		int right = arr.length-1; 
		int i=0;
		while(i<arr.length) {
			if(arr[i]==0) {
				int temp = arr[left];
				arr[left] = arr[i];
				arr[i] = temp;
				left++;
				i++;
				
//				System.out.println(temp);
//				System.out.println(left);
				
			}else if(arr[i]==1) {
				int temp = arr[right];
				arr[right] = arr[i];
				arr[i] = temp;
				right--;
				i++;
			
//				System.out.println(temp);
//				System.out.println(right);
			}else if(arr[i]==2) {
				i++;
			}
		}
		
		for(int j=0 ; j<arr.length ; j++) {
			System.out.print(arr[j]+" ");
		}
	}
}
